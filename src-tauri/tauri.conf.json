{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Studioklocka",
  "version": "0.1.0",
  "identifier": "com.leorehnstrom.studio-timepiece",
"build": {
  "beforeDevCommand": "npm run dev",
  "beforeBuildCommand": "npm run build",
  "frontendDist": "../dist",
  "devUrl": "http://localhost:8080"
}

,
  "app": {
    "windows": [
      {
        "title": "Studioklocka",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/mamenex/studio-timepiece/releases/latest/download/latest.json"
      ],
      "dialog": false,
      "pubkey": "RWVoCQE7Oc1HNnUvoVC6Rc/co/an58TFIkavNw+20lW8lIynX7ZJxV1X"
    }
  }
}
